{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/sitemap.xml",
      "dest": "/public/sitemap.xml"
    },
    {
      "src": "/terms",
      "dest": "/terms"
    },
    {
      "src": "/privacy",
      "dest": "/privacy"
    }
  ]
}

